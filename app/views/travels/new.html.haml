= #breadcrumb :new_offer_season_travel, @travel

.region-texto

  %h1.page-title
    =t('travel.new')

  = form_for(@travel, :url => {:travel_id => params[:travel_id], :action => 'create'}, :html => { :multipart => true}) do |f|
    = render(:partial => 'form', :locals => {:f => f})
    .actions
      = f.submit I18n.t('submit')

